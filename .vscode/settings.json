{
    "Lua.runtime.version": "LuaJIT",
    "Lua.workspace.library": [
        "${3rd}/love2d/library"
    ],
    "Lua.workspace.checkThirdParty": false,
    "Lua.diagnostics.disable": [
        "redundant-parameter",
        "different-requires",
        "undefined-field",
        "undefined-global"
    ],
    "Lua.diagnostics.globals": [
        "_INTERNALPATH",
        "_UTILITYPATH",
        "_COMPONENTPATH",
        "_IMAGEPATH",
        "Vector"
    ]
}